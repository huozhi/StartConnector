				<EasingDoubleKeyFrame KeyTime="0" Value="23.25"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.4" Value="5.25"/>
				<EasingDoubleKeyFrame KeyTime="0:0:0.8" Value="0"/>
			</DoubleAnimationUsingKeyFrames>

	</UserControl.Resources>
	<UserControl.Triggers>
		<EventTrigger RoutedEvent="FrameworkElement.Loaded">
			<BeginStoryboard x:Name="ShakeLeft_BeginStoryboard" Storyboard="{StaticResource ShakeLeft}"/>
			<BeginStoryboard Storyboard="{StaticResource ShakeRight}"/>
		</EventTrigger>
	</UserControl.Triggers>

	<Canvas x:Name="LayoutRoot">
		<Image x:Name="image" Height="183.5" Canvas.Left="180" Source="newnet.png" Stretch="Fill" Canvas.Top="286.5" Width="662.5" RenderTransformOrigin="0.5,0.5">
			<Image.RenderTransform>
				<TransformGroup>
					<ScaleTransform/>
					<SkewTransform/>
					<RotateTransform/>
					<TranslateTransform/>
				</TransformGroup>
			</Image.RenderTransform>
		</Image>
	</Canvas>
</UserControl>